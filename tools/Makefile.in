CC=@CC@
RM=@RM@
TCMLIBS=@LIBS@
INCLUDEDIR=-I../include

.c.o:
	${CC} ${CPPFLAGS} ${INCLUDEDIR} ${CFLAGS} -c $<

all: mkpasswd

build: all

mkpasswd: mkpasswd.o
	$(CC) $(LDFLAGS) -o ../mkpasswd mkpasswd.o ${TCMLIBS}

clean:
	${RM} -f *.o *~ *core

distclean: clean
	${RM} -f Makefile

depend:

lint:
	lint -aacgprxhH ${CPPFLAGS} ${INCLUDEDIR} mkpasswd.c >>../lint.out
